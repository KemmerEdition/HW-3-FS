{
  "name": "fast_speech_second_test",
  "n_gpu": 1,
  "arch": {
    "type": "FastSpeechSecond",
    "args": {
    }
  },
      "datasets":
        {
          "type": "BufferDataset",
          "args": {
          }
        },
  "dataloaders": {
    "type": "LJLoader",
    "args": {
      "batch_size": 34,
      "batch_expand_size": 8,
      "num_workers": 0
    }
  },
  "optimizer": {
    "type": "AdamW",
    "args": {
      "lr": 1e-3,
      "betas": [0.9, 0.98],
      "eps": 1e-9,
      "weight_decay": 1e-6
    }
  },
  "loss": {
    "type": "FS_2Loss",
    "args": {}
  },
  "lr_scheduler": {
    "type": "OneCycleLR",
    "args": {
      "steps_per_epoch": 396,
      "epochs": 600,
      "anneal_strategy": "cos",
      "max_lr": 1e-3,
      "pct_start": 0.1
    }
  },
  "trainer": {
    "epochs": 600,
    "save_dir": "saved/",
    "save_period": 10,
    "verbosity": 2,
    "monitor": "min val_loss",
    "early_stop": 100,
    "visualize": "wandb",
    "wandb_project": "fast_speech_project",
    "len_epoch": 396,
    "grad_norm_clip": 1.0
  }}

